//When course is the owning side of the relationship
SELECT * FROM COURSE_ENROLLED_BY_STUDENTS ,STUDENT ,COURSE WHERE COURSE_ENROLLED_BY_STUDENTS .STUDENT_ID = STUDENT.ID AND COURSE_ENROLLED_BY_STUDENTS .COURSE_ID  = COURSE.ID

//When Student is the owning side of the relationship
SELECT * FROM STUDENT_ENROLLED_COURSES ,STUDENT ,COURSE WHERE STUDENT_ENROLLED_COURSES.STUDENT_ID = STUDENT.ID AND STUDENT_ENROLLED_COURSES.COURSE_ID  = COURSE.ID